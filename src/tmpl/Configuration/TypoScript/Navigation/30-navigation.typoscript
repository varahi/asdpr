# lib.navigation.submenu
lib.navigation = HMENU
lib.navigation {

    wrap = <div class="wrap">|</div>
    #special = directory
    #special.value = {$config.rootPageId}

    1 = TMENU
    1.NO.doNotShowLink = 1

    2 = TMENU
    2 {
        NO.linkWrap = <li>|</li>

        ACT = 1
        ACT.linkWrap = <li class="active">|</li>

        wrap = <ul class="left-menu">|</ul>
    } # 2 end

}
